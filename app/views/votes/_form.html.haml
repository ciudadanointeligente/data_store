= form_for @vote do |f|
  - if @vote.errors.any?
    #error_explanation
      %h2= "#{pluralize(@vote.errors.count, "error")} prohibited this vote from being saved:"
      %ul
        - @vote.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :uid
    = f.text_field :uid
  .field
    = f.label :chamber
    = f.time_select :chamber
  .field
    = f.label :bill_id
    = f.text_field :bill_id
  .actions
    = f.submit 'Save'
